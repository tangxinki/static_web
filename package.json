{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "serve": "npm run dev",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts && npm run zip",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts && npm run zip",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean": "rimraf node_modules",
    "zip": "esno build/script/compress2zip.ts",
    "report": "rimraf dist && cross-env vite build",
    "test": "vitest",
    "preview": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@iconify/iconify": "^3.1.1",
    "@vueuse/core": "^10.2.1",
    "archiver": "^6.0.0",
    "axios": "^1.4.0",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "echarts": "^5.4.3",
    "element-plus": "^2.4.1",
    "esno": "^0.16.3",
    "fs-extra": "^9.1.0",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "picocolors": "^1.0.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vant": "^4.6.8",
    "vue": "^3.3.4",
    "vue-router": "^4.1.6",
    "vue-types": "^5.1.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.116",
    "@purge-icons/generated": "^0.9.0",
    "@types/archiver": "^5.3.2",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "autoprefixer": "^10.4.14",
    "core-js": "^3.31.1",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.15.1",
    "postcss": "^8.4.29",
    "postcss-px-to-viewport": "^1.1.1",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.64.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.13.0",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.5"
  }
}